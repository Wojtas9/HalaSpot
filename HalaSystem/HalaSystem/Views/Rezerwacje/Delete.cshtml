@model HalaSystem.Models.Rezerwacja

@{
    ViewData["Title"] = "Delete";
}

<h1>Usuwanie rezerwacji</h1>

<h3>Czy jesteś pewien, że chcesz to Usunąć?</h3>
<div>
    <hr />
    <h4>Rezerwacja</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.UserId)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UserId)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Od)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Od)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Do)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Do)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Hala)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Hala.Id)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Delete" class="btn btn-danger" />
        <div class="mt-3">
            <a asp-action="Index" class="btn btn-warning">
                <i class="bi bi-backspace"></i> Wróć do listy
            </a>
        </div>
    </form>
</div>
